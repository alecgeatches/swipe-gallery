{
    "apiVersion": 2,
    "name": "alecg/swipe-gallery",
    "title": "Swipe Gallery",
    "icon": "screenoptions",
    "category": "media",
    "editorScript": "alecg-swipe-gallery-editor-js",
    "editorStyle": "file:./css/editor.css",
    "style": "alecg-swipe-gallery-frontend-css",
    "script": "alecg-swipe-gallery-frontend-js",
    "attributes": {
        "images": {
            "type": "array",
            "default": []
        },
        "aspectRatio": {
            "enum": [
                "16-9",
                "4-3",
                "3-2",
                "1-1"
            ],
            "default": "3-2"
        },
        "desktopItemsPerRow": {
            "type": "integer",
            "default": 4
        },
        "mobileItemsPerRow": {
            "type": "integer",
            "default": 2
        }
    }
}
